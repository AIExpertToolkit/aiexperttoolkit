---
layout: null
permalink: /sitemap.xml
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% assign host = site.url | append: site.baseurl %}
  {% for page in site.pages %}
    {% assign path = page.url | default: page.path %}
    {% unless page.sitemap == false or page.url contains 'sitemap.xml' or page.url contains 'robots.txt' or page.url contains 'assets/' or page.name == '404.html' %}
      <url>
        <loc>{{ host }}{{ page.url | replace:'index.html','' }}</loc>
        {% if page.last_modified_at %}
          <lastmod>{{ page.last_modified_at | date_to_xmlschema }}</lastmod>
        {% else %}
          <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
        {% endif %}
        <changefreq>weekly</changefreq>
        <priority>{% if page.url == '/' or page.url == '/aiexperttoolkit/' %}1.0{% else %}0.6{% endif %}</priority>
      </url>
    {% endunless %}
  {% endfor %}
</urlset>
